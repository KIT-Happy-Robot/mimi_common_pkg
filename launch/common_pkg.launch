<launch>
    <node pkg="mimi_common_pkg" name="enter_the_room" type="enter_the_room.py"/>
    <node pkg="mimi_common_pkg" name="localize_object" type="localize_object.py"/>
    <node pkg="mimi_common_pkg" name="approach_person" type="approach_person.py"/>
    <node pkg="mimi_common_pkg" name="setup_location" type="setup_location.py">
      <rosparam file="$(find mimi_common_pkg)/config/location_dict.yaml" command="load" ns="location_dict" />
    </node>
    <rosparam file="$(find mimi_common_pkg)/config/object_mapping.yaml" command="load" ns="object_mapping" />
</launch>
